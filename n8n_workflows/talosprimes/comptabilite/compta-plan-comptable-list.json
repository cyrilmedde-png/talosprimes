    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT id, numero_compte, libelle, classe, type, nature, compte_parent, actif, solde_debit, solde_credit FROM plan_comptable WHERE tenant_id = $1 AND actif = true AND ($2 IS NULL OR classe = $2) AND ($3 IS NULL OR nature = $3) AND ($4 IS NULL OR (numero_compte ILIKE '%' || $4 || '%' OR libelle ILIKE '%' || $4 || '%')) ORDER BY numero_compte ASC;",
