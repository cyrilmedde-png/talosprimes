        700,
        300
      ]
    },
    {
      "parameters": {
        "jsCode": "const items = $('05. Query').all().map(item => item.json).filter(c => c.id);\nconst comptes = items.map(c => ({\n  id: c.id,\n  numeroCompte: c.numero_compte,\n  libelle: c.libelle,\n  classe: c.classe,\n  type: c.type,\n  nature: c.nature,\n  compteParent: c.compte_parent,\n  soldeDebit: parseFloat(c.solde_debit) || 0,\n  soldeCredit: parseFloat(c.solde_credit) || 0,\n  solde: (parseFloat(c.solde_debit) || 0) - (parseFloat(c.solde_credit) || 0)\n}));\nreturn [{ json: { success: true, comptes, count: comptes.length } }];"
      },
