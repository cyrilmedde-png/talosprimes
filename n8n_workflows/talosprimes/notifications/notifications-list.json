        "query": "={{ \"SELECT id, tenant_id, type, titre, message, donnees, lu, created_at, updated_at FROM notifications WHERE tenant_id = '\" + $('02. Parser').first().json.tenantId + \"' AND \" + ($('02. Parser').first().json.lu !== null && $('02. Parser').first().json.lu !== undefined ? $('02. Parser').first().json.lu : null ? \"lu = '\" + $('02. Parser').first().json.lu !== null && $('02. Parser').first().json.lu !== undefined ? $('02. Parser').first().json.lu : null + \"'::boolean\" : \"1=1\") + \" ORDER BY created_at DESC LIMIT \" + $('02. Parser').first().json.limit + \" OFFSET \" + $('02. Parser').first().json.offset }}"
