{
  "name": "twilio-config-update",
  "nodes": [
    {
      "parameters": {
        "path": "twilio-config-update",
        "responseMode": "responseNode",
        "httpMethod": "POST"
      },
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        250,
        250
      ]
    },
    {
      "parameters": {
        "jsCode": "const raw = $input.first().json;\nconst _wb = (raw && raw.body && typeof raw.body === \"object\") ? raw.body : raw;\nconst body = (_wb && _wb.data && typeof _wb.data === \"object\") ? { ..._wb.data, tenantId: _wb.data.tenantId || _wb.tenantId } : _wb;\nreturn {\n  id: body.id,\n  tenantId: body.tenantId,\n  accountSid: body.accountSid,\n  authToken: body.authToken,\n  phoneNumber: body.phoneNumber,\n  isActive: body.isActive !== undefined ? body.isActive : true\n};"
      },
      "name": "Parser",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        450,
        250
      ]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [
            {
              "value1": "={{ $json.id }}",
              "operation": "notEmpty"
            },
            {
              "value1": "={{ $json.tenantId }}",
              "operation": "notEmpty"
            }
          ]
        }
      },
      "name": "If",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        650,
        250
      ]
    },
    {
      "parameters": {
        "respondWithCode": 400,
        "responseBody": "{{ { error: 'Missing required fields: id, tenantId', success: false } }}"
      },
      "name": "Respond Error",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [
        850,
        150
      ]
    },
    {
      "parameters": {
        "host": "{{ $env.DB_HOST }}",
        "database": "{{ $env.DB_NAME }}",
        "user": "{{ $env.DB_USER }}",
        "password": "{{ $env.DB_PASSWORD }}",
        "port": 5432,
        "operation": "executeQuery",
        "query": "UPDATE twilio_configs SET {{ $json.accountSid !== undefined ? \"account_sid = '\" + $json.accountSid + \"',\" : \"\" }} {{ $json.authToken !== undefined ? \"auth_token = '\" + $json.authToken + \"',\" : \"\" }} {{ $json.phoneNumber !== undefined ? \"phone_number = '\" + $json.phoneNumber + \"',\" : \"\" }} {{ $json.isActive !== undefined ? \"is_active = \" + ($json.isActive ? \"true\" : \"false\") + \",\" : \"\" }} updated_at = NOW() WHERE id = '{{ $json.id }}' AND tenant_id = '{{ $json.tenantId }}'"
      },
      "name": "Postgres Update",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2,
      "position": [
        850,
        250
      ],
      "credentials": {
        "postgres": {
          "name": "Postgres"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "return {\n  configId: $json.id,\n  phoneNumber: $json.phoneNumber,\n  isActive: $json.isActive,\n  success: true,\n  message: 'Twilio config updated successfully'\n};"
      },
      "name": "Format Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1050,
        250
      ]
    },
    {
      "parameters": {
        "responseBody": "{{ $json }}"
      },
      "name": "Respond",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [
        1250,
        250
      ]
    }
  ],
  "connections": {
    "Webhook": {
      "main": [
        [
          {
            "node": "Parser",
            "branch": 0,
            "index": 0,
            "type": "main"
          }
        ]
      ]
    },
    "Parser": {
      "main": [
        [
          {
            "node": "If",
            "branch": 0,
            "index": 0,
            "type": "main"
          }
        ]
      ]
    },
    "If": {
      "main": [
        [
          {
            "node": "Postgres Update",
            "branch": 0,
            "index": 0,
            "type": "main"
          }
        ],
        [
          {
            "node": "Respond Error",
            "branch": 0,
            "index": 0,
            "type": "main"
          }
        ]
      ]
    },
    "Postgres Update": {
      "main": [
        [
          {
            "node": "Format Response",
            "branch": 0,
            "index": 0,
            "type": "main"
          }
        ]
      ]
    },
    "Format Response": {
      "main": [
        [
          {
            "node": "Respond",
            "branch": 0,
            "index": 0,
            "type": "main"
          }
        ]
      ]
    }
  }
}
