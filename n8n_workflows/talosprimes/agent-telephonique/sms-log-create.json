        250,
        250
      ]
    },
    {
      "parameters": {
        "jsCode": "var body = $input.first().json.body || $input.first().json;\nreturn [{ json: {\n  id: require('crypto').randomUUID(),\n  tenantId: body.tenantId,\n  leadId: body.leadId || null,\n  phoneNumber: body.phoneNumber,\n  message: body.message,\n  status: body.status || 'sent',\n  direction: body.direction || 'outbound',\n  templateId: body.templateId || null,\n  campaignId: body.campaignId || null,\n  sentAt: body.sentAt || new Date().toISOString(),\n  deliveredAt: body.deliveredAt || null,\n  failureReason: body.failureReason || null\n} }];"
      },
      "name": "Parser",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        450,
        250
      ]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [
            {
              "value1": "={{ $json.tenantId }}",
              "operation": "notEmpty"
            },
            {
              "value1": "={{ $json.phoneNumber }}",
              "operation": "notEmpty"
            }
          ]
        }
      },
      "name": "If",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        650,
        250
      ]
    },
    {
      "parameters": {
        "respondWithCode": 400,
        "responseBody": "={{ { success: false, error: 'Missing required fields: tenantId, phoneNumber' } }}"
      },
      "name": "Respond Error",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [
        850,
        150
      ]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "={{ \"INSERT INTO sms_logs (id, tenant_id, lead_id, phone_number, message, status, direction, template_id, campaign_id, sent_at, delivered_at, failure_reason, created_at, updated_at) VALUES ('\" + $json.id + \"', '\" + $json.tenantId + \"', '\" + $json.leadId + \"', '\" + $json.phoneNumber + \"', '\" + $json.message + \"', '\" + $json.status + \"', '\" + $json.direction + \"', '\" + $json.templateId + \"', '\" + $json.campaignId + \"', '\" + $json.id + \"'0, '\" + $json.id + \"'1, '\" + $json.id + \"'2, NOW(), NOW())\" }}"
      },
      "name": "Postgres Insert",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2,
      "position": [
        850,
        250
      ],
      "credentials": {
        "postgres": {
          "name": "Supabase Postgres",
          "id": "OGqj65ZoFRileCck"
        }
      },
      "alwaysOutputData": true
    },
    {
      "parameters": {
        "jsCode": "return [{ json: {\n  smsLogId: $json.id,\n  phoneNumber: $json.phoneNumber,\n  status: $json.status,\n  sentAt: $json.sentAt,\n  success: true,\n  message: 'SMS log created successfully'\n} }];"
      },
      "name": "Format Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1050,
