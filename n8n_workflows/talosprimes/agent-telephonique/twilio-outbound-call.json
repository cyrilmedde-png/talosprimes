{
  "name": "twilio-outbound-call",
  "nodes": [
    {
      "parameters": {
        "path": "twilio-outbound-call",
        "responseMode": "responseNode",
        "httpMethod": "POST"
      },
      "id": "01. Webhook",
      "name": "01. Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        100,
        200
      ]
    },
    {
      "parameters": {
        "jsCode": "const payload = $input.body;\n\nif (!payload.tenantId) {\n  throw new Error('tenantId is required');\n}\n\nif (!payload.to) {\n  throw new Error('to (phone number) is required');\n}\n\nif (!payload.reason) {\n  throw new Error('reason is required');\n}\n\nreturn {\n  tenantId: payload.tenantId,\n  to: payload.to,\n  reason: payload.reason\n};"
      },
      "id": "02. Parser",
      "name": "02. Parser",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        300,
        200
      ]
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{ $json.tenantId }}",
              "operation": "isNotEmpty"
            }
          ]
        }
      },
      "id": "03. Valide ?",
      "name": "03. Valide ?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        500,
        200
      ]
    },
    {
      "parameters": {
        "respondWithOptions": {
          "responseType": "json"
        },
        "responseBody": "{\n  \"success\": false,\n  \"error\": \"Validation failed: tenantId is required\"\n}"
      },
      "id": "04. Respond Error",
      "name": "04. Respond Error",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [
        700,
        100
      ]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT id, tenant_id, phone_number, agent_name, company_name, niche, active FROM twilio_configs WHERE tenant_id = $1",
        "queryParameters": "={{ [$json.tenantId] }}"
      },
      "id": "05. Get Twilio Config",
      "name": "05. Get Twilio Config",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2,
      "credentials": {
        "postgres": {
          "name": "Supabase Postgres",
          "id": "OGqj65ZoFRileCck"
        }
      },
      "position": [
        700,
        300
      ]
    },
    {
      "parameters": {
        "jsCode": "const results = $input.all();\nconst config = results.length > 0 ? results[0].json : null;\nconst to = $('02. Parser').first().json.to;\nconst reason = $('02. Parser').first().json.reason;\nconst tenantId = $('02. Parser').first().json.tenantId;\n\nif (!config) {\n  return {\n    success: false,\n    error: 'Aucune configuration Twilio trouvée pour ce tenant',\n    canProceed: false\n  };\n}\n\nif (!config.active) {\n  return {\n    success: false,\n    error: 'La configuration Twilio est désactivée',\n    canProceed: false\n  };\n}\n\nreturn {\n  tenantId: tenantId,\n  to: to,\n  reason: reason,\n  fromNumber: config.phone_number,\n  agentName: config.agent_name,\n  companyName: config.company_name,\n  canProceed: true\n};"
      },
      "id": "06. Check Config",
      "name": "06. Check Config",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        900,
        300
      ]
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{ $json.canProceed }}",
              "operation": "isNotEmpty"
            }
          ]
        }
      },
      "id": "07. Config OK ?",
      "name": "07. Config OK ?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        1100,
        300
      ]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO call_logs (tenant_id, caller_phone, direction, status, notes, urgency_level, sentiment, action_taken, created_at, updated_at) VALUES ($1, $2, 'sortant', 'initiated', 'Appel sortant: $3', 'STANDARD', 'NEUTRE', 'APPEL', NOW(), NOW()) RETURNING id, tenant_id, caller_phone, direction, status, notes, created_at",
        "queryParameters": "={{ [$json.tenantId, $json.to, $json.reason] }}"
      },
      "id": "08. Create Call Log",
      "name": "08. Create Call Log",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2,
      "credentials": {
        "postgres": {
          "name": "Supabase Postgres",
          "id": "OGqj65ZoFRileCck"
        }
      },
      "position": [
        1300,
        350
      ]
    },
    {
      "parameters": {
        "jsCode": "const callLog = $input.all();\nconst params = $('06. Check Config').first().json;\n\nif (callLog.length === 0) {\n  return {\n    success: false,\n    error: 'Erreur lors de la création du log d appel'\n  };\n}\n\nreturn {\n  success: true,\n  message: 'Outbound call initiated successfully',\n  data: {\n    callLogId: callLog[0].json.id,\n    to: params.to,\n    from: params.fromNumber,\n    reason: params.reason,\n    agentName: params.agentName,\n    companyName: params.companyName,\n    status: 'initiated'\n  }\n};"
      },
      "id": "09. Format Response",
      "name": "09. Format Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1500,
        350
      ]
    },
    {
