        "operation": "executeQuery",
        "query": "={{ \"SELECT COUNT(*) as succeeded FROM event_logs WHERE tenant_id = '\" + $json.tenantId + \"' AND statut_execution = 'succes' AND \" + ($json.workflow ? \"workflow = '\" + $json.workflow + \"'\" : \"1=1\") }}"
