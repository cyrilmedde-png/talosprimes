        "query": "={{ \"SELECT COUNT(*) as en_attente FROM event_logs WHERE tenant_id = '\" + $json.tenantId + \"' AND statut_execution = 'en_attente' AND \" + ($json.workflow ? \"workflow = '\" + $json.workflow + \"'\" : \"1=1\") }}"
      },
