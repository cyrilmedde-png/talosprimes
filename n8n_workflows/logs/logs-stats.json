      "parameters": {
        "code": "const totalResult = $('05. Count Total').first();\nconst succeededResult = $('06. Count Succeeded').first();\nconst errorsResult = $('07. Count Errors').first();\nconst pendingResult = $('08. Count Pending').first();\nconst byWorkflowData = $('09. Count By Workflow').all();\n\nconst byWorkflow = {};\nbyWorkflowData.forEach(item => {\n  byWorkflow[item.workflow] = item.count;\n});\n\nreturn {\n  total: totalResult.total,\n  succeeded: succeededResult.succeeded,\n  errors: errorsResult.errors,\n  enAttente: pendingResult.en_attente,\n  byWorkflow: byWorkflow\n};"
