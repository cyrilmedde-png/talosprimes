      "parameters": {
        "jsCode": "// Préparer les URLs de redirection pour Stripe Checkout\nconst prepareData = $('01. Préparer données onboarding').item.json;\nconst baseUrl = 'https://talosprimes.com';\nconst clientId = prepareData.clientId;\n\n// URLs de redirection après paiement\nconst successUrl = `${baseUrl}/clients?checkout=success&clientId=${clientId}&session_id={CHECKOUT_SESSION_ID}`;\nconst cancelUrl = `${baseUrl}/clients?checkout=cancelled&clientId=${clientId}`;\n\nreturn {\n  json: {\n    ...prepareData,\n    checkoutSuccessUrl: successUrl,\n    checkoutCancelUrl: cancelUrl\n  }\n};"
      },
