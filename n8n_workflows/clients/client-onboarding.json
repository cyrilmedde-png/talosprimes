    {
      "parameters": {
        "jsCode": "// Préparer la réponse de succès\nconst subscription = $input.item.json;\nconst clientData = $('01. Préparer données onboarding').item.json.clientData;\nconst prepareData = $('08. Préparer avec IDs Stripe').item.json || $('01. Préparer données onboarding').item.json;\n\n// Si Stripe est activé, on a une URL de checkout\nconst stripeData = prepareData.avecStripe && prepareData.checkoutUrl ? {\n  customerId: prepareData.idClientStripe,\n  checkoutSessionId: prepareData.idSessionCheckoutStripe,\n  checkoutUrl: prepareData.checkoutUrl,\n  requiresPayment: true\n} : null;\n\nreturn {\n  json: {\n    success: true,\n    message: prepareData.avecStripe && prepareData.checkoutUrl \n      ? \"Espace client créé. Redirection vers le paiement...\" \n      : \"Espace client créé avec succès\",\n    client: clientData,\n    subscription: subscription[0] || subscription,\n    modulesActives: subscription[0]?.modules_inclus || subscription.modules_inclus || [],\n    stripe: stripeData\n  }\n};"
      },
      "id": "format-response",
      "name": "11. Formater réponse",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1560, 500]
    },
    {
      "parameters": {
        "jsCode": "// Préparer le body JSON pour la notification\nconst prepareData = $('01. Préparer données onboarding').item.json;\nconst responseData = $('11. Formater réponse').item.json;\nconst subscription = responseData.subscription;\n\nconst clientName = prepareData.clientData.nom || prepareData.clientData.prenom || prepareData.clientData.raisonSociale || 'le client';\nconst titreStripe = prepareData.avecStripe ? ' (Stripe)' : '';\nconst messageStripe = prepareData.avecStripe ? ' - Abonnement Stripe créé' : '';\n\nreturn {\n  json: {\n    type: 'client_onboarding',\n    titre: `Espace client créé${titreStripe}`,\n    message: `L'espace client et l'abonnement ont été créés avec succès pour ${clientName}${messageStripe}`,\n    donnees: {\n      tenantId: prepareData.tenantId,\n      clientId: prepareData.clientId,\n      subscriptionId: subscription?.id || subscription?.[0]?.id\n    }\n  }\n};"
      },
